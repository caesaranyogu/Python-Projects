def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
while at_goal() == False:
    while front_is_clear() and at_goal() == False:
        move()   
        while right_is_clear():
            turn_right()
            move()
            turn_right()
            move ()
    while wall_in_front():
        turn_left()             
